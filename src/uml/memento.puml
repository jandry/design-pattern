@startuml Memento

class Morceau {
    - nom: String
    + creerMemento(): MementoMorceau
    + restaurer(MementoMorceau)
}

class MementoMorceau {
    - nom: String
}

class LecteurMusique {
    - historiqueMorceau: List<MementoMorceau>
    + lire(Morceau): String
    + lireMorceauPrecedent(): String
}

@enduml